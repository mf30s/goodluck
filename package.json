{
  "name": "yo",
  "version": "1.0.0",
  "description": "personal",
  "author": "M Fachri <fachri@gredu.asia>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
